{
    "manifest_version": 2,
    "name": "Click Capture",
    "version": "1.0",
    "description": "Capture clicks on any webpage",
    "permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  